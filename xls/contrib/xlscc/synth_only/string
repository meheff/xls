#ifndef __STRING_H__
#define __STRING_H__

namespace std {
class string {
 public:
  string() {(void)__xlscc_unimplemented();}
  string(const char *s) {(void)__xlscc_unimplemented();}

  string &operator += (const string&rhs) {(void)__xlscc_unimplemented();return *this;}
  string &operator += (char rhs) {(void)__xlscc_unimplemented();return *this;}

};

string operator + (const std::string& lhs, char rhs) {return lhs;}

}  // namespace std

#endif//__STRING_H__
